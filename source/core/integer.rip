type {
	@.+ = -> (other) {
		@.type.+(@, other)
	}

	@.- = -> (other) {
		@.type.-(@, other)
	}

	@.* = -> (other) {
		@.type.*(@, other)
	}

	@./ = -> (other) {
		@.type./(@, other)
	}

	@.% = -> (other) {
		@.type.%(@, other)
	}

	@./% = -> (other) {
		@.type./%(@, other)
	}

	self./% = -> (a, b) {
		d = a / b
		r = a % b
		[ d, r ]
	}

	@.to_integer = -> { @ }

	@.to_rational = -> { @.type.to_rational(@) }

	to_string = -> { 'System.Integer' }
}
